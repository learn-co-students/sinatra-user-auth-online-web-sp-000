<h1>Sign Up Below:</h1>

<form action="/registrations" method="post">
  <p><label for="user_name">Name: </label>
  <input type="text" name="name" id="user_name"></p>

  <p><label for="user_email">Email Address: </label>
  <input type="text" name="email" id="user_email"></p>

  <p><label for="user_password">Password: </label>
  <input type="text" name="password" id="user_password"></p>

  <p><input type="submit" value="Sign Up"></p>
</form>
